{{#if site.keys.googleAnalytics}}
{{#with site.keys.googleAnalytics}}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{this}}"></script>
    <script>dataLayer=window.dataLayer||[];function gtag(){dataLayer.push(arguments)};gtag('js',new Date());gtag('config','{{this}}')</script>
{{/with}}
{{/if}}
{{#if (eq site.url "https://docs.rudder.io")}}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@docsearch/css@3" />
    <!-- set Rudder blue as main color -->
    <style>
    :root{--docsearch-primary-color:#13beb7;}
    </style>
{{/if}}
{{#if site.url}}
    <link rel="icon" href="{{site.url}}/_/img/favicon.ico" type="image/x-icon">
{{else}}
    <link rel="icon" href="/rudder-doc/_/img/favicon.ico" type="image/x-icon">
{{/if}}
